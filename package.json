{
  "name": "file-manager",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --port 5000",
    "fix:all": "run-s fix:prettier fix:stylelint fix:eslint",
    "fix:eslint": "eslint --fix ./src/**/*.{svelte,js,jsx,ts,tsx}",
    "fix:prettier": "prettier --write --plugin-search-dir=. ./src/**/*.{svelte,js,jsx,ts,tsx,css,scss,saas}",
    "fix:stylelint": "stylelint --fix ./src/**/*.{svelte,css,scss,saas}",
    "lint:all": "run-s lint:prettier lint:stylelint lint:eslint",
    "lint:eslint": "eslint ./src/**/*.{svelte,js,jsx,ts,tsx}",
    "lint:prettier": "prettier --check --plugin-search-dir=. ./src/**/*.{svelte,js,jsx,ts,tsx,css,scss,saas}",
    "lint:stylelint": "stylelint ./src/**/*.{svelte,css,scss,saas}",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "test": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "1.22.2",
    "@sveltejs/adapter-node": "1.0.0-next.81",
    "@sveltejs/kit": "1.0.0-next.372",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "4.0.0",
    "html-minifier": "4.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-html": "1.5.0",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "rimraf": "3.0.2",
    "sass": "1.53.0",
    "stylelint": "14.9.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended-scss": "7.0.0",
    "stylelint-config-standard": "26.0.0",
    "stylelint-scss": "4.3.0",
    "svelte": "3.44.0",
    "svelte-check": "2.7.1",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.3.1",
    "typescript": "4.7.4",
    "vite": "2.9.13"
  }
}
